<h1><%= table_header %></h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">Class</th>
      <th scope="col">Instructor</th>
      <th scope="col">Partners</th>
      <th scope="col">Tags</th>
      <th scope="col">Summary</th>
    </tr>
  </thead>
  <tbody>
    <% @notes.each do |note| %>
    <tr>
      <td><%= format_date(note) %><br><div class="notetitle"><%= format_title(note) %></div></td>
      <td><%= note.instructor_list %></td>
      <td><%= note.partner_list %></td>
      <td><%= format_tags(note)%></td>
      <td><%= note.pos_description %></td>
      <td><%= link_to "view", edit_note_path(note) %></td>
    </tr>
    <% end %>
  </tbody>
</table>