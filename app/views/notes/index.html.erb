<h1>notes/index</h1>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Class</th>
      <th scope="col">Date</th>
      <th scope="col">Instructors</th>
      <th scope="col">Partners</th>
      <th scope="col">Summary</th>
    </tr>
  </thead>
  <tbody>
    <tr>
    <% @notes.each do |note| %>
      <th scope="row"><%= note.note_title_list %></th>
      <td><%= note.note_date %></td>
      <td><%= note.instructor_list %></td>
      <td><%= note.partners %></td>
      <td><%= note.pos_description %></td>
    <% end %>
    </tr>
  </tbody>
</table>