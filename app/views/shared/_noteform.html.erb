<%= form_for @note do |f| %>
<%= f.date_field :note_date%>
<%= f.text_field :note_title_list, placeholder: "class title" %>
<%= f.text_field :instructor_list, placeholder: "instructor_list" %>
<%= f.text_field :tag_list, placeholder: "tag1, tag2" %>
<%= f.text_field :partners, placeholder: "partner1, partner2" %>
<%= f.text_area :pos_description, placeholder: "summary of what went well" %>
<%= f.text_area :neg_description, placeholder: "summary of what can be improved" %>
 <%= f.submit "Save changes" %>
<% end %>